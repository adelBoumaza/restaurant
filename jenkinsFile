pipeline {
    agent any
    stages {
        stage('Compilation Stage') {
            withMaven(maven:'apache-maven-3.5.3') {
              sh 'mvn clean compile'
            }
        }
        stage('Test Stage') {
            withMaven(maven:'apache-maven-3.5.3') {
              sh 'mvn test'
            }
        }
        stage('Depoly stage') {
            withMaven(maven:'apache-maven-3.5.3') {
              sh 'mvn deploy'
            }
        }
    }
}
