
pipeline {
    agent any
    stages {
           stage('Compilation Stage') {
                steps {
                    withMaven(maven:'apache-maven-3.5.3') {
                      sh 'mvn clean compile'
                    }
                }
            }
        
        
            stage('Test Stage') {
                steps {
                    withMaven(maven:'apache-maven-3.5.3') {
                      sh 'mvn test'
                    }
                }
            }

            stage('Depoly stage') {
                 steps {
                    withMaven(maven:'apache-maven-3.5.3') {
                      sh 'mvn install'
                    }
                  }
            }
    }
}
